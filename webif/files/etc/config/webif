config webif general
	option firmware_name ""
	option firmware_version ""
	option firmware_subtitle "With X-Wrt Extensions"
	option device_name ""
	
config webif syslog
	option ipaddr ""
	option port ""
	option size ""
	option type ""
	option mark ""
	option file ""

config webif cron
	option enable "1"
	
